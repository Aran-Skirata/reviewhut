<% layout('./layout/boilerplate') %>
<h3>new place</h3>
<form action="/places" method="POST">
  <input type="text" name="place[title]" placeholder="Place title" />
  <input type="text" name="place[price]" placeholder="Price" />
  <input type="text" name="place[description]" placeholder="description" />
  <input type="text" name="place[location]" placeholder="location" />
  <button type="submit">Submit</button>
</form>
<a href="/">Cancel</a>
